nav(ng-controller="LoginCtrl")
    ul
        li(ng-show="loggedIn && userControl")
            a(href="/users") Users
        li(ng-show="loggedIn && editPages")
            a(href="/pages") Pages
        li(ng-show="loggedIn")
            a(href="/themes") Themes
        li(ng-hide="loggedIn")
            a(href="/login") Login
        li(ng-show="loggedIn")
            a(href ng-click="logout();") Logout